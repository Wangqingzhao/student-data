<table class="table table-striped table-hover ">
    <thead>
        <th>姓名</th>
        <th id="age-head" onclick="ageHeadClick()">年龄(点击可排序)</th>
        <th>性别</th>
        <th>电话</th>
        <th>编辑</th>
        <th>删除</th>
    </thead>
    <%data.forEach(function(s,i){%>
    <tr>
        <td><%=s.name%></td>
        <td><%=s.age%></td>
        <td><%=s.gender%></td>
        <td><%=s.tel%></td>
        <td>
            <span class="glyphicon glyphicon-edit cursor-pointer" stu-ind="<%=i%>" onclick="editBtnClick(event)"></span>
        </td>
        <td>
            <span 
                class="glyphicon glyphicon-trash stu-delete" 
                stu-id="<%=s._id%>"
                onclick="deleteBtnClick(event)"
            ></span>
        </td>
    </tr>
    <%})%>
</table>